<html>
    <head>
        <link rel="extends" href="./type"/>
        <link rel="tag" href="gutentag/choose.html"/>
        <link rel="tag" href="./mode.html"/>
        <meta accepts="[body]" as="arg"/>
    </head>
    <body>
        <choose id="view">
            <active><span class="element active"><em>?</em></span></active>
            <any>any type</any>
            <number>number</number>
            <string>string</string>
            <boolean>boolean</boolean>
            <array>
                <sp>array of </sp>
                <this id="of"></this>
            </array>
            <map>
                <sp>map from </sp>
                [<this id="key"></this>]
                <sp> to </sp>
                <this id="value"></this>
            </map>
